class Simulation
{
public:
	int num_of_masses, int num_of_models;
	Ball **	masses;
	Model ** models;

	Simulation(int num_of_masses, int num_of_models, float mass)
	{
		this->num_of_masses = num_of_masses;
		this->num_of_models = num_of_models;

		masses = new Ball*[num_of_masses];
		models = new Model*[num_of_models];

		for (int a=0; a<num_of_masses; a++)
		{
            masses[a] = new Ball(mass, Vec3d(500, 0, 500));
			models[a] = new Model(mass, Vec3d(0, 0, 0));
		}
	}
	virtual void toast()
	{
		for(int a=0; a<num_of_masses; a++)
		{
			delete masses[a];
			delete models[a];
			masses[a] = NULL;
			models[a] = NULL;
		}
		delete(masses);
		masses = NULL;
		delete(models);
		models = NULL;
	}

	Mass *getMass(int index)
	{
		if(index<0 || index>=num_of_masses)
			return NULL;
		return masses[index];
	}
	
	Model *getModel(int index)
	{
		if(index<0 || index>=num_of_models)
			return NULL;
		return models[index];
	}
	virtual void init()
	{
		//Init all masses
		
		for(int i=0; i<num_of_masses; i++)
		{
			//masses[i]->init();
			//masses[i]->Render();
			models[i]->init();
			models[i]->Render();
		}
	}

	virtual void solve()
	{
	}

	virtual void simulate(float dtime)
	{
		for(int i=0; i<num_of_masses; i++)
		{
			masses[i]->simulate(dtime);
			models[i]->simulate(dtime);
		}

	}

	virtual void operate(float dtime)
	{
		//The main function which calls all the relevant functions to simulate stuff
		init();
		solve();
		simulate(dtime);
	}
};

