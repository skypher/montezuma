  (pif (null nullstr) (csetq nullstr \"\"))
  (pcond
    ((null trm) (ret nullstr))
    ((consp trm) (ret (cconcatenate commastr (funcall fun (car trm)) (prolog-cdr-to-string fun (cdr trm) commastr barstr nullstr))))
