;; peek-char that can emit #\\Null
(define pch ()
  (pif (null (peek-char nil *standard-input* nil)) (ret (code-char 0)))
  (ret (peek-char)))
;; unread-char that accepts #\\Null
(define uch (c)
  (pif (char/= c #\\Null) (unread-char c)))
;; read-char that can emit #\\Null
(define nch ()
  (pif (null (peek-char nil *standard-input* nil)) (ret (code-char 0)))
  (ret (read-char)))
;; read-char (non-white) that can emit #\\Null
(define rch ()
  (pif (null (peek-char nil *standard-input* nil)) (ret (code-char 0)))
  (clet ((ch (nch))) 
        (ret (fif (member ch *WHITESPACE-CHARS*) (rch) ch ))))


(define charvarp (ch) (ret (cor (upper-case-p ch) (char= ch #\\_) (char= ch #\\?) )))
(define alphanum (ch) (ret (cor (alphanumericp ch) (charvarp ch))))
(define valdigit (ch) (ret (digit-char-p ch)))
 
(define read_number (ch)
  (ret (CDO ((v (valdigit ch) (+ (* v 10) (valdigit (nch)))))
      ((cnot (digit-char-p (pch))) v))))

(define read_number (ch)
    (uch ch)
    (ret (read)))

(define read_atom (ch)
  (pif (char= ch #\\Null ) (ret :EOF))
  (clet ((op (infix-op ch))) (fif op (ret op)))
  (ret (CDO ((lch (list ch) (cpush (nch) lch)))
      ((cnot (alphanum (pch))) (implode (reverse lch))))))

(define read_qatom (ch)
  (ret (CDO ((lch (list ch) (cpush (nch) lch)))
      ((char= (pch) #\\') (nch) (implode (reverse lch))))))

(define read_string (ch)
  (ret (CDO ((lch (list (char-code ch)) 
     (cpush (char-code (nch)) lch)))
      ((char= (pch) #\\\" ) (nch ) (do_char_list (reverse lch))))))      
    (define do_char_list (trm)
    (ret (fif (atom trm) trm (list '|.| (car trm) (do_char_list (cdr trm))))))

(define read_string (ch)
    (uch ch)
    (clet ((str (read)))
        (ret str)))


(define make_var_ref (trm)
 (funless (CYC-VAR? trm) (csetq trm (MAKE-EL-VAR trm)))
 (csetq trm (CORRECT-VARIABLE trm ))
 ;; (myprint (list :make_var_ref trm))
 (funless (member trm *lvarglob)(cpushnew trm *lvarloc))
 (ret trm))

(define read_atomic (ch)
  (pif (char= ch #\\Null)  (ret :EOF))
  (clet ((op (infix-op ch))) (fif op (ret op)))
  (pcond
   ((char= ch #\\{) (ret (cons '|{}| (read_comma_list (rch) #\\}))))
   ;;((char= ch #\\() (ret (cons '|,| (read_comma_list (rch) #\\)))))
   ;;((char= ch #\\%) (readto \\#Newline)(ret (read_atomic (rch))))
   ((charvarp ch) (ret (make_var_ref (read_atom ch))))
   ((valdigit ch) (ret (read_number ch)))
   ((char= ch #\\\" ) (ret (prolog-make-string (read_string (nch)))))
   ((cand (char= ch #\\# )(member (pch) '(#\\S ))) (read-char ch)(ret (read)))
   ((char= ch #\\# )(uch ch)(ret (read)))
 ;;((cand (char= ch #\\# )(member (pch) '(#\\$ #\\\\ #\\( ))) (unread-char ch)(ret (read)))
   ((char= ch #\\` )(ret (read-char ch)))
   ((char= ch #\\[ ) (ret (read_list (rch) )))
   ((char= ch #\\' ) (ret (read_qatom (nch))))
   ( t (ret (read_atom ch)))))

(define prolog-make-string (str)(ret str))

(define read_term (ch &optional lastch)
  (CLET ((trm nil) (c nil))
    (csetq trm (read_atomic ch))
    (csetq c (rch))
    ;;(myprint c)
    (pif (member c '(#\\Null #\\.)) (ret trm))
    (pif (char= c #\\( ) 
        (clet ( (rrest (read_comma_list (rch) #\\) ) ))
           (csetq trm (prolog-make-clause trm rrest))
           (csetq c (rch))))
    (pif (member c '(#\\Null #\\.)) (ret trm))
    (clet ((op (infix-op c))) 
       (pif op (ret (list op trm (read_term (rch) )))))
    ;;(myprint c)
    (uch c) 
    (ret trm)
    ))

(define prolog-make-clause (pred rrest &optional mt)
    (ret (cons *prologClause* (cons (prolog-to-predicate pred (length rrest)) rrest))))    

(define charstring (&rest chars) 
   ;; (print chars)(force-output)
    (ret (implstring (mapcar #'char-code chars))))

(define infix-op (c &optional list)
    (pif (null list) (csetq list (append *xfy* *xfx* *fx* *fy*)))
    (clet ((opstr \"\"))
        #|(clet ((nc (nch)))
            (csetq opstr (charstring c nc (pch)))
            (pif (member opstr list #'equal) (progn (nch)(ret (intern opstr))))
            (uch nc))|#
        (csetq opstr (charstring c (pch)))
        (pif (member opstr list #'equal) (progn (nch)(ret (intern opstr))))
        (csetq opstr (charstring c))
        (pif (member opstr list #'equal) (ret (intern opstr)))))

