chomp_([$\\r|T]) -> chomp_(T);
chomp_(S) -> S.

lowercase([C|T]) when C =< $Z, C >= $A -> [C-$A+$a|lowercase(T)];
lowercase([C|T]) -> [C|lowercase(T)];
lowercase([]) -> [].
