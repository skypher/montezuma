(defmacro defprolog (name farity args &rest body)
  (let ((arity (length `,args))(fname (concatenate 'string `,name \"/\" (write-to-string `,farity))))
  (let ((sym (intern `,fname)))
    (let ((fort (prolog-to-predicate `,fname `,arity)))
     (setf (get `,sym 'evaluable) t)
     (setf (get `,sym 'prolog_name) `,name)
     (setf (get `,sym 'prolog_arity) `,farity)
     (setf (get `,sym 'lisp_arity) `,arity)
      (prolog-assert-now (list (find-or-create-constant \"arity\") `,fort `,arity ) *prologMt* ) 
      (prolog-assert-now (list (find-or-create-constant \"evaluationDefn\") `,fort (list (find-or-create-constant \"SubLQuoteFn\") `,sym)) *prologMt* ) 
      `(define-evaluation-defn ,sym ,args (ret (progn . ,body)))))))

(define prolog-predicatep (fort)
    (ret (cand (fort-p fort) 
     (cor (predicate? fort)(cyc-query (list #$isa fort *prologPrediateType*) *prologMt*)))))


(define prolog-first-clause (pred &optional mt)
    (ret (first-answer (ask-template '?first (list *prologFirst* pred '?first) (fif mt mt *prologMt*)))))

(define prolog-last-clause (pred &optional mt)
    (clet ((cloz (prolog-first-clause pred mt))(nxtcl cloz))
     (cdo  () ((null nxtcl) (ret cloz))
            (csetq nxtcl (prolog-next-clause cloz mt)))))

(define prolog-next-clause (cloz &optional mt)
    (ret (first-answer (ask-template '?next (list *prologNext* cloz '?next) (fif mt mt *prologMt*)))))

(define first-answer (ansrs)
  (ret (fif (consp ansrs) (car ansrs))))
        
(define prolog-assertz (sent &optional mt)
   (clet ((mt (fif mt mt *prologMt*))(pred (prolog-to-predicate sent))(laast (prolog-last-clause pred))(nxt (prolog-to-assertion sent mt)))
         (pcond 
            ((null laast) (prolog-assert-now (list *prologFirst* pred nxt) mt))
            (t (prolog-assert-now (list *prologNext* laast nxt) mt)))
      (ret nxt)))


