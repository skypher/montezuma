+    var minor = \"+\";
+  } else {
+    var minor = parseInt(bits[1]);
+    // If minor is Not a Number (NaN) return an empty string
+    if (isNaN(minor)) {
+      minor = \"\";
+    }
+  }
+  return {major: parseInt(bits[0]), minor: minor, is_nightly: is_nightly};
