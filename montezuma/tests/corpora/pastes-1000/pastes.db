    if [[ $pc == \"\" ]] ; then
        echo \"nothing found for $*\"
        return 1
    fi

    cd ${d}/${pc}
}

# is a box alive?
isalive() {
    ping -c1 $1
    return $?
}

# run on all boxes
runall() {
    local s
    for s in $(<${HOME}/servers ) ; do
        if [[ `isalive $s` ]] ; then
            echo -e \"$(hostcolour $s)${s}\\033[00m\"
            ssh $s $*
        fi
    done
}

eshowkw() {
