(defun send-mail-using-server (server from to message
		    &key cc bcc subject reply-to headers)
  ;;
  ;; see documentation at the head of this file
  ;;
  (let ((header (make-string-output-stream))
	(tos
	 (cond
	   ((stringp to) 
	    (list to)) 
	   ((consp to)
	    to)
	   (:not-a-list-or-string
	    (error \"to should be a string or list, not ~s\" to))))
	(ccs
	 (cond
	   ((null cc)
	    nil)
	   ((stringp cc) 
	    (list cc) )
	   ((consp cc)
	    cc)
	   (:not-a-list-or-string
	    (error \"cc should be a string or list, not ~s\" cc))))
	(bccs
	 (cond
	   ((null bcc)
	    nil)
	   ((stringp bcc) 
	    (list bcc))
	   ((consp bcc)
	    bcc)
	   (:not-a-string-or-list
	    (error \"bcc should be a string or list, not ~s\" bcc)))))
    (format header \"From: ~a~c~cTo: \"
	    from
	    #\\return
	    #\\linefeed)
    (format header \"~{ ~a~^,~}~c~c\" tos #\\return #\\linefeed)
    (when ccs 
     (format header \"Cc: ~{ ~a~^,~}~c~c\" ccs #\\return #\\linefeed))
    
    (when subject
     (format header \"Subject: ~a~c~c\" subject #\\return #\\linefeed))
    
    (when reply-to
      (format header \"Reply-To: ~a~c~c\" reply-to #\\return #\\linefeed))
    
    (when headers
      (cond ((stringp headers)
	     (setq headers (list headers)))
	     ((consp headers)
	       t)
	     (:header-format-error
	      (error \"Unknown headers format: ~s.\" headers)))
      (dolist (h headers) 
	(format header \"~a~c~c\" h #\\return #\\linefeed)))
    
    (format header \"~c~c\" #\\return #\\linefeed)
    
    (send-smtp server from (append tos ccs bccs)
	       (get-output-stream-string header)
	       message)))


