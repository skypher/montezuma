(eval-when (:compile-toplevel)
(defun generate-galois-tables ()
  (let* ((primitive-polynomial #O435)
         (maximum (expt 2 8))
         (log-values (make-array maximum :element-type (list 'integer 0 maximum)))
         (inverse-log-values (make-array maximum :element-type (list 'integer 0 maximum)))
         (value 1))
    (loop for i from 0 below (1- maximum)
          do
          (setf (aref log-values value) i
                (aref inverse-log-values i) value
                value (+ value value))
          (when (>= value maximum) (setf value (logxor value primitive-polynomial))))
    (values log-values inverse-log-values)))
)

(defvar *multiplication-table* 
  (multiple-value-bind (log-values inverse-log-values)
      (generate-galois-tables)
    (let ((multiplication-table (make-array '(256 256) :element-type '(unsigned-byte 8))))
      (loop for i from 0 below 256
            do
            (loop for j from 0 below 256
                  do
                  (setf (aref multiplication-table i j) 
                        (if (or (zerop i) (zerop j))
                            0
                          (let ((sum-logs (+ (aref log-values i)
                                             (aref log-values j))))
                            (when (>= sum-logs 255) (decf sum-logs 255))
                            (aref inverse-log-values sum-logs))))))
      multiplication-table)))

(defvar *division-table*
  (multiple-value-bind (log-values inverse-log-values)
      (generate-galois-tables)
    (let ((division-table (make-array '(256 256) :element-type '(unsigned-byte 8))))
      (loop for i from 0 below 256
            do
            (loop for j from 0 below 256
                  do
                  (setf (aref division-table i j)
                        (if (or (zerop i) (zerop j))
                            0
                          (let ((diff-logs (- (aref log-values i)
                                              (aref log-values j))))
                            (when (< diff-logs 0) (incf diff-logs 255))
                            (aref inverse-log-values diff-logs))))))
      division-table)))

(defmacro in-galois-field (&rest forms)
  `(macrolet ((add (x y) `(logxor ,x ,y))
              (sub (x y) `(logxor ,x ,y))
              #-lispworks
              (mul (x y) `(aref *multiplication-table* ,x ,y))
              #+lispworks
              (mul (x y) `(locally (declare (type fixnum ,x ,y) (optimize (speed 3) (safety 0))) (system:svref-no-check$i-vector-8$fixnum *multiplication-table* (the fixnum (+ (the fixnum (* 256 ,x)) ,y)))))
              (div (x y) `(aref *division-table* ,x ,y)))
     (labels ((pow (x e) (cond ((zerop e) 1)
                               ((= e 1) x)
                               ((evenp e) (pow (mul x x) (/ e 2)))
                               ((oddp e) (mul x (pow (mul x x) (floor e 2)))))))
       ,@forms)))
  
(defun vandermonde (rows cols)
  (let ((result (make-array (list rows cols))))
    (in-galois-field 
     (loop for i from 0 below rows
           do
           (loop for j from 0 below cols
                 do
                 (setf (aref result i j) (pow (1+ j) i)))))
    result))

(defun make-correlation-matrix (data-specs check-specs)
  (let* ((number-of-data (length data-specs))
         (number-of-checks (length check-specs))
         (matrix (make-array (list number-of-data number-of-data) :initial-element 0))
         (vandermonde (vandermonde number-of-checks number-of-data))
         (current-row 0))
    (loop for data-item in data-specs
          for index from 0
          when data-item
          do
          (setf (aref matrix current-row index) 1)
          (incf current-row))
    (loop for check-item in check-specs
          for index from 0
          while (< current-row number-of-data)
          when check-item
          do
          (loop for j from 0 below number-of-data do (setf (aref matrix current-row j)
                                                           (aref vandermonde index j)))
          (incf current-row))
    (assert (= current-row number-of-data))
    matrix))

(defun invert-matrix (matrix)
  (let* ((rows (array-dimension matrix 0))
         (cols (array-dimension matrix 1))
         (result (make-array (list rows cols) :initial-element 0)))
    (assert (= rows cols))
    ;; set up result as unity matrix
    (loop for i from 0 below rows
          do
          (setf (aref result i i) 1))
    ;; do Gaussian elimination on both arrays
    (in-galois-field 
     (flet ((find-pivot (index) (loop for i from index below rows
                                      unless (zerop (aref matrix i index))
                                      do (return i)))
            (exchange-rows (row-1 row-2)
                           (loop for i from 0 below rows
                                 do
                                 (rotatef (aref matrix row-1 i) (aref matrix row-2 i))
                                 (rotatef (aref result row-1 i) (aref result row-2 i))))
            (gaussian-eliminate (index)
                                (let* ((pivot (aref matrix index index))
                                       (inverse-pivot (div 1 pivot)))
                                  ;; normalize row
                                  (loop for i from 0 below rows
                                        do
                                        (setf (aref matrix index i) (mul (aref matrix index i) inverse-pivot)
                                              (aref result index i) (mul (aref result index i) inverse-pivot)))
                                  ;; eliminate
                                  (loop for i from 0 below rows
                                        for element = (aref matrix i index)
                                        unless (= i index)
                                        do
                                        (loop for j from 0 below rows
                                              do
                                              (setf (aref matrix i j)
                                                    (sub (aref matrix i j)
                                                         (mul element (aref matrix index j))))
                                              (setf (aref result i j)
                                                    (sub (aref result i j)
                                                         (mul element (aref result index j)))))))))
       (loop for i from 0 below rows
             for pivot-index = (find-pivot i)
             unless (= i pivot-index)
             do
             (exchange-rows i pivot-index)
             do
             (gaussian-eliminate i))))
    result))
                                                  
                            
