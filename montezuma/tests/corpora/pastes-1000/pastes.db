   ;;((nart-p trm) (ret (prolog-to-nart trm)))
   ((dotted-list-p trm) (ret (prolog-to-cdr trm)))
   ((consp trm) 
     (pif (equal (car trm) '|.|) (ret (prolog-from-list trm)))
