				}
					
				//$proxiedLinks[html_entity_decode($urls[$i][\"_content\"])] = $url;				
				$proxiedLinks[$urls[$i][\"_content\"]] = $url;				
			}	
		}	
	}
	return $proxiedLinks;
}

