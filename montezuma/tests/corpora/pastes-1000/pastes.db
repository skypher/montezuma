(define prolog-to-formula (formula &optional mt)
  (pcond 
    ((el-variable-p formula) (ret (list *prologClause* . formula)))
    ((prolog-predicatep formula) (ret formula))
    ((cnot (consp formula)) (ret (list *prologClause* formula)))
    (t (clet ((pred (elt formula 0)))
       (pcond
        ((equal pred #$ist) 
            (ret (prolog-to-formula (elt formula 2) (elt formula 1))))
        ((equal pred '|:-|) 
            (pif (= (length formula) 2) (ret (prolog-to-formula (elt formula 1) mt)))
            (ret (cons *entailedBy* (mapcar #'prolog-to-formula (cdr formula)))))
        ((equal pred '|.|) (ret (prolog-from-list formula)))
        ((prolog-predicatep pred) (ret formula))
        (t 
          (ret (cons *prologClause* 
            (cons (prolog-to-predicate formula) (prolog-to-cdr (cdr formula)))))))))))

(define prolog-to-predicate (formula &optional arity)
  ;; (print (list 'prolog-to-predicate (list 'quote formula)))
   (pcond
    ((el-variable-p formula) (ret formula))
    ((prolog-predicatep formula) (ret formula))
    ((atom formula)(ret (list *prologPrediateFn* (prolog-to-name formula) (fif arity arity '??ARITY))))
    ((dotted-list-p formula) (ret (prolog-to-predicate (car formula) '??)))
    (t
     (clet ((pred (elt formula 0)))
      (fif (equal '|:-| pred) (ret (prolog-to-predicate (elt formula 1))))
      (fif (equal #$ist pred) (ret (prolog-to-predicate (elt formula 2))))
     ;; (print (list 'prolog-predicatep  (list 'quote pred)))
      (fif (prolog-predicatep pred) (ret pred))
      ;;(print (list 'length (list 'quote formula)))
      (fif (null arity) (csetq arity (- (length formula) 1)))
      (ret (list *prologPrediateFn* (prolog-to-name pred) arity))))))


(define prolog-to-name (trm &optional mt)
    (pif (cand (consp trm)(equal (elt trm 0) *prologPrediateFn*)) (ret (elt trm 1)))
    (pif (constant-p trm) (ret (cconcatenate \"#$\" (prolog-to-name (constant-name trm)))))
    (fif (symbolp trm) (csetq trm (symbol-name trm)))
    (funless (stringp trm) (csetq trm (write-to-string trm)))
    (ret trm))


(define prolog-to-cdr (trm &optional mt)
  (pcond
    ((null trm) (ret trm))
    ((consp trm) (ret (cons (prolog-to-cycl (car trm) mt) (prolog-to-cdr (cdr trm) mt))))
    (t (ret (prolog-to-cycl trm mt)))))


(define prolog-to-variable (trm &optional mt)
    (pif (el-variable-p trm)(csetq trm (EL-VAR-NAME-WITHOUT-PREFIX trm)))
    (funless (stringp trm) (csetq trm (write trm)))
    (cdo ((idx 0 (+ 1 idx))) ((= idx (length trm))  (ret  trm )) 
        (pcase (char trm idx)
          (#\\- (set-char trm idx #\\_)))))    
    
(define prolog-to-nart (trm &optional mt)
    (pif (nart-p trm) (csetq trm (nart-el-formula trm)))
    (ret (list \"nart(\" (prolog-to-cycl (car trm)) (prolog-to-cdr (cdr trm) \",\" \"|\" \"\") \")\")))

(define prolog-to-constant (trm &optional mt)
    (pcond
        ((cand (constant-p trm) (function? trm)) (ret (list \"fn:\" (prolog-to-atom trm))))
        (t (ret (list \"cycl:\" (prolog-to-atom trm))))))

