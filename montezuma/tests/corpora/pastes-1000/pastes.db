(define read_comma_list (ch lchar)
 (pif (member ch (cons lchar '(#\\Null #\\.))) (ret ()))
  (clet ((*xfy* (remove \",\" *xfy* #'equal))(*xfx* (remove \",\" *xfx* #'equal))(trm (read_term ch)) (c (rch)))
    (pif (char= c lchar) (ret (list trm)))
    (pif (char= c #\\,) (ret (cons trm (read_comma_list (rch) lchar))))
    (progn (uch c) (ret (list trm)))))

   
(define read_list (ch)
  (pif (char= ch #\\] ) (ret NIL))
  (pif (char= ch #\\Null ) (ret NIL))
    (CLET ((trm (read_term ch))(ch (rch)))
      (PCASE ch
	    (#\\, (ret (list '|.| trm (read_list (rch)))))
	    (#\\| (ret (list '|.| trm (read_term (rch)))))
	    (#\\] (ret (list '|.| trm nil))))
    (print \"bad character\")
    (print ch)
        ))

(define read_clause (ch) 
  (CLET ((trm (read_term ch)) (c (rch)))
    (pif (char= c #\\.) (ret trm))
    (pif (char= c #\\:) (nch))
    (ret (list '|:-| trm (read_comma_list (rch) #\\.)))))

(define ccc (l)
  (ret 
    (fif (atom l) 
        (fif (member l *lvarloc) 
            (cons 'L (position l *lvarloc))
        (fif (member l *lvarglob) 
            (cons 'G (position l *lvarglob)) l))
       (fif (eq (car l) '! ) (list '! (length *lvarloc))
      (cons (ccc (car l)) (ccc (cdr l)))))))

